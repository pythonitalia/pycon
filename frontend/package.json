{
  "name": "pycon-italia",
  "version": "1.0.0",
  "author": "Patrick Arminio <patrick.arminio@gmail.com>",
  "scripts": {
    "build:gatsby": "gatsby build",
    "build:docz": "docz build",
    "build": "yarn build:gatsby",
    "develop": "gatsby develop -p 4000",
    "start": "concurrently -k -n 'gatsby,codegen' -c 'bgBlue.bold,bgMagenta.bold' 'yarn develop' 'yarn codegen:watch'",
    "format": "prettier --write \"{,!(node_modules|public|coverage|.cache)/**/}*.{js,jsx,ts,tsx,json}\" && yarn lint --fix",
    "lint": "eslint --fix --ext .jsx,.js,.tsx,.ts src/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "codegen-fragments": "graphql-codegen -r dotenv/config -c codegen-fragments.yml",
    "codegen-queries": "graphql-codegen -r dotenv/config -c codegen.yml",
    "codegen": "yarn codegen-fragments && yarn codegen-queries",
    "codegen:watch": "concurrently -k -n 'fragments,queries' -c 'bgGreen.black.bold,bgYellow.black.bold' 'yarn codegen-fragments --watch'  'yarn codegen-queries --watch'",
    "precommit-lint": "node scripts/lint",
    "postinstall": "cd plugins/gatsby-source-pycon && yarn"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@reach/router": "^1.2.1",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/reach__router": "^1.2.6",
