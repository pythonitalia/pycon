{
  "trailingSlash": false,
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=0, s-maxage=300, stale-while-revalidate"
        }
      ]
    },
    {
      "source": "/:lang*/profile",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/:lang*/profile/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/login/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/complete/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/:lang*/login/success",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/graphql",
      "destination": "https://d2gn2ji624wbmi.cloudfront.net/graphql"
    },
    {
      "source": "/admin",
      "destination": "https://d2gn2ji624wbmi.cloudfront.net/admin/"
    },
    {
      "source": "/admin/:match*",
      "destination": "https://d2gn2ji624wbmi.cloudfront.net/admin/:match*/"
    },
    {
      "source": "/django-static/:match*",
      "destination": "https://d2gn2ji624wbmi.cloudfront.net/django-static/:match*"
    }
  ],
  "redirects": [
    {
      "source": "/login/google",
      "destination": "https://d2gn2ji624wbmi.cloudfront.net/login/google-oauth2/",
      "permanent": false
    },
    {
      "source": "/complete/google-oauth2",
      "destination": "https://d2gn2ji624wbmi.cloudfront.net/complete/google-oauth2/",
      "permanent": false
    }
  ]
}
